{
  "title": "new user",
  "description": "describes properties required to create a user",
  "type": "object",
  "properties": {
    "firstName": {
      "type": "string",
      "description": "firstname of the account user"
    },
    "lastName": {
      "type": "string",
      "description": "lastname of the account user"
    },
    "email": {
      "type": "string",
      "description": "username of account"
    },
    "role": {
      "type": "string",
      "description": "User role",
      "enum": ["Student", "Collaborator", "Creator"]
    },
    "password": {
      "type": "string",
      "minLength": 8,
      "description": "password of the account user"
    }
  },
  "required": ["firstName", "lastName", "email", "role", "password"]
}